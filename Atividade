package com.mycompany.test;
public class Test {

    public static void main(String[] args) {
        int idade = 30;
        float preco1 = 2100.00F;
        float  preco2 = 650.50F;
        char sexo = 'f';
        String codigo = "5290";
        
      
        System.out.printf("Produtos: /n Computador, o preço é: %.2f", preco1);
        System.out.printf("Mesa de escritório, o preço é: %.2f", preco2);
        System.out.printf("Registro: %d anos de idade, codigo %s e sexo:%c", idade, codigo, sexo);
        System.out.printf("Medida com oito casas decimais: ");
        System.out.printf("Arredondando(três casas decimais): ");
        System.out.printf("Decimal usado nos EUA: ");
    }
}
